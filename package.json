{
  "name": "root-module",
  "version": "0.0.2",
  "description": "Find root module with path and package.json",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "yoda": [
        "off"
      ]
    },
    "globals": {
      "$": false
    }
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "root",
    "module",
    "root module",
    "module root",
    "node_modules",
    "package",
    "package.json",
    "root package"
  ],
  "author": "N. Palani Kumanan",
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/root-module.git"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1"
  }
}
